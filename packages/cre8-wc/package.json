{
  "name": "@cre8_dev/cre8-wc",
  "version": "1.0.5",
  "description": "cre8 Web Components is a library of presentational UI web components to be consumed by Cigna web applications.",
  "scripts": {
    "start": "pnpm run build:custom-elements.json && pnpm run storybook",
    "build": "del-cli lib && vite build && tsc",
    "build:watch": "vite build --watch",
    "build:custom-elements.json": "npx @custom-elements-manifest/analyzer analyze --litelement --globs \"components/*/*.ts\" --outdir .storybook",
    "test": "jest --verbose",
    "test:jenkins": "jest --coverage=false --ci --runInBand --silent --bail",
    "storybook-docs": "storybook dev --docs",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "del-cli dist && storybook build -o dist",
    "plop": "plop component",
    "extract-tokens": "node ./extract-tokens.cjs https://mcdonalds.com",
    "convert-scss-to-ts": "npx tsx ./scripts/convert-scss-to-ts.ts",
    "agentrpc": "npx tsx ./cli/agentric-server.ts",
    "generate-theme": "npx tsx ./cli/theme-generator.ts",
    "theme-manager": "npx tsx ./cli/theme-manager.ts",
    "theme-validator": "npx tsx ./cli/theme-validator.ts",
    "plop:page": "plop page",
    "plop:recipe": "plop recipe",
    "lint": "eslint-config --ext .ts,.html . --ignore-path ../../.eslintignore",
    "format": "eslint-config --ext .ts,.html . --fix --ignore-path ../../.eslintignore"
  },
  "type": "module",
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "",
  "files": [
    "dist",
    "lib",
    ".yalc"
  ],
  "exports": {
    ".": "./lib/index.js",
    "./lib/components/*": "./lib/components/*.js",
    "./lib/design-tokens/*": "./lib/design-tokens/*",
    "./lib/directives/*": "./lib/directives/*",
    "./lib/icons/*": "./lib/icons/*",
    "./lib/svgs/*": "./lib/svgs/*",
    "./lib/index.js": "./lib/index.js"
  },
  "dependencies": {
    "@a11y/focus-trap": "^1.0.5",
    "@cre8_dev/cre8-design-tokens": "1.0.3",
    "agentrpc": "^0.0.16",
    "chart.js": "^4.4.9",
    "classnames": "2.3.2",
    "lit": "3.3.0",
    "lit-html": "^2.0.0-rc.1",
    "nanoid": "^3.1.30",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@custom-elements-manifest/analyzer": "^0.10.4",
    "@mdx-js/react": "^3.1.0",
    "@mdx-js/rollup": "^3.1.0",
    "@storybook/addon-a11y": "9.1.0-alpha.1",
    "@storybook/addon-docs": "9.1.0-alpha.1",
    "@storybook/addon-essentials": "9.0.0-alpha.12",
    "@storybook/addon-themes": "9.1.0-alpha.1",
    "@storybook/builder-vite": "9.0.2",
    "@storybook/react": "^9.0.2",
    "@storybook/web-components": "^9.0.2",
    "@storybook/web-components-vite": "^9.0.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^29.5.0",
    "@types/jest-axe": "^3.5.9",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/sanitize-html": "^2.9.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@whitespace/storybook-addon-html": "^7.0.0",
    "babel-jest": "^29.5.0",
    "clone": "^1.0.4",
    "css-parse": "2.0.0",
    "del-cli": "^4.0.1",
    "dotenv": "^16.0.0",
    "element-internals-polyfill": "^1.3.7",
    "fs": "0.0.1-security",
    "glob": "^10.3.10",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.5.0",
    "js-yaml": "^3.14.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^11.0.1",
    "loader-utils": "^3.2.0",
    "marked": "^15.0.12",
    "openai": "^4.96.0",
    "path": "^0.12.7",
    "plop": "^2.7.4",
    "postcss": "^8.5.3",
    "puppeteer": "^24.0.0",
    "react": "^19.1.0",
    "react-docgen-typescript": "^2.2.2",
    "react-dom": "^19.1.0",
    "sanitize-html": "^2.11.0",
    "node-sass": "9.0.0",
    "storybook": "^9.0.2",
    "style-dictionary": "^3.1.1",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-lit-plugin": "^2.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
