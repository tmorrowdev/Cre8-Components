{
  "name": "@cre8/cre8-wc",
  "version": "1.0.0-beta",
  "description": "cre8 Web Components is a library of presentational UI web components to be consumed by Cigna web applications.",
  "scripts": {
    "start": "pnpm build:custom-elements.json && pnpm storybook",
    "build": "del-cli lib && webpack && tsc",
    "build:watch": "webpack --watch",
    "build:custom-elements.json": "npx web-component-analyzer analyze \"components/*/*.ts\" --outFile .storybook/custom-elements.json",
    "test": "jest --verbose",
    "test:jenkins": "jest --coverage=false --ci --runInBand --silent --bail",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "del-cli dist && storybook build -o dist",
    "plop": "plop component",
    "plop:page": "plop page",
    "plop:recipe": "plop recipe",
    "lint": "eslint-config --ext .ts,.html . --ignore-path ../../.eslintignore",
    "format": "eslint-config --ext .ts,.html . --fix --ignore-path ../../.eslintignore"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "",
  "files": [
    "dist",
    "lib",
    ".yalc"
  ],
  "exports": {
    ".": {
      "node": "./lib/index.js",
      "default": "./lib/index.js"
    },
    "./lib/components/*": "./lib/components/*.js",
    "./lib/design-tokens/*": "./lib/design-tokens/*",
    "./lib/directives/*": "./lib/directives/*",
    "./lib/icons/*": "./lib/icons/*",
    "./lib/svgs/*": "./lib/svgs/*",
    "./lib/theme.js": "./lib/theme.js"
  },
  "dependencies": {
    "@a11y/focus-trap": "^1.0.5",
    "@cre8/cre8-design-tokens": "file:../../cre8-cre8-design-tokens-1.0.0.tgz",
    "@cre8/cre8-icons": "file:../../cre8-cre8-icons-0.6.0.tgz",
    "classnames": "2.3.2",
    "lit": "^2.0.0-rc.1",
    "lit-html": "^2.0.0-rc.1",
    "nanoid": "^3.1.30"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.14.5",
    "@babel/plugin-transform-typescript": "^7.14.6",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@etchteam/storybook-addon-css-variables-theme": "^2.0.6",
    "@etchteam/storybook-addon-status": "^4.2.4",
    "@storybook/addon-a11y": "^7.5.3",
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/theming": "^7.5.3",
    "@storybook/web-components": "^7.5.3",
    "@storybook/web-components-webpack5": "^7.5.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/jest": "^29.5.0",
    "@types/jest-axe": "^3.5.9",
    "@types/resize-observer-browser": "^0.1.7",
    "@types/sanitize-html": "^2.9.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@whitespace/storybook-addon-html": "^5.1.6",
    "babel-jest": "^29.5.0",
    "babel-loader": "^8.2.2",
    "clone": "^1.0.4",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "6.7.4",
    "del-cli": "^4.0.1",
    "element-internals-polyfill": "^1.3.7",
    "file-loader": "^6.2.0",
    "glob": "^7.1.7",
    "jest": "^29.5.0",
    "jest-canvas-mock": "^2.5.2",
    "jest-environment-jsdom": "^29.5.0",
    "js-yaml": "^3.14.1",
    "jsdom": "^22.1.0",
    "json-loader": "^0.5.7",
    "lint-staged": "^11.0.1",
    "loader-utils": "^3.2.0",
    "markdown-loader": "^8.0.0",
    "mini-css-extract-plugin": "^2.2.1",
    "plop": "^2.7.4",
    "raw-loader": "^4.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "sanitize-html": "^2.11.0",
    "sass": "1.55.0",
    "sass-loader": "^13.3.1",
    "storybook": "^7.5.3",
    "style-dictionary": "^3.1.1",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-lit-plugin": "^2.0.1",
    "typescript": "^5.2.2",
    "web-component-analyzer": "^2.0.0",
    "webpack": "^5.43.0",
    "webpack-cli": "^4.7.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix"
  }
}
